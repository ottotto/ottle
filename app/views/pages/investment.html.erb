<script src="https://rawgithub.com/laff/technical-indicators/master/technical-indicators.src.js"></script>


<div class="container">
	<head>
		<h1>Investment Mentality<span class="text-muted"> - Under Construction</span></h1>
	</head>
	<body id="stockgraphs">

        <div id="inputSymbol" class="row"> 
                
                <div class="col-md-12 form-group">
                    <p>Enter Stock</p>
                    <input id="txtSymbol" class="required" Placeholder="Symbol" />   
                    <input id="startDate" class="datePick required" type="text"  Placeholder="From" />   
                    <input id="endDate" class="datePick" type="text" Placeholder="To"  />
                </div>
                <div class="col-md-12 form-group">
                    <select id="durSel" class="required form-control" style="height: 20px; width: 95px">
                      <option value="daily">Daily</option>
                      <option value="weekly">Weekly</option>
                      <option value="monthly">Monthly</option>
                    </select>
                </div>
                <div class="col-md-12 form-group">
                    <p>Moving Average</p>
                    <input id="stperiod" class="required" type="text" Placeholder="ST Period (default 50)" />   
                    <input id="ltperiod" class="required" type="text" Placeholder="LT Period (default 200)" />   
                </div>
                <div class="col-md-12 form-group">
                    <select id="durSelma" class="required form-control" style="height: 20px; width: 95px">
                      <option value="sma">SMA</option>
                      <option value="ema">EMA</option>
                    </select>
                </div>
               <div class="col-md-12">
                    <button id="submit" class="btn btn-warning btn-sm">Submit</button> 
                </div>
            </div>   
            <br>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Realtime Bid</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><span id="symbol"></span></td> 
                    <td><span id="bidRealtime"></span></td>
                </tr>
            </tbody>
        </table>   

        <div id="containera"></div>
        <br>
        <div id="containerb"></div>
        <br>
        <div id="containerlive"></div>
	</body>
    <div style="height:300px"></div>
</div>
